	<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,Log4net" />
  </configSections>
<log4net>
		<root>
			<level value="ALL" />
			<appender-ref ref="InfoRollingLogFileAppender" />
			<appender-ref ref="ErrorRollingLogFileAppender" />
		</root>
		<!--Errores-->
		<appender name="ErrorRollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
			<filter type="log4net.Filter.LevelRangeFilter">
				<param name="levelMin" value="ERROR" />
			</filter>
			<lockingModel type="LogAppender.MinimalLockDeleteEmpty" />
			<file value="C:\Log\AMPAExt\Error_" />
			<datePattern value="dd.MM.yyyy'.txt'" />
			<staticLogFileName value="false" />
			<appendToFile value="true" />
			<rollingStyle value="Composite" />
			<maxSizeRollBackups value="10" />
			<maximumFileSize value="10MB" />
			<layout type="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" value="[%date{dd/MM/yyyy HH:mm:ss fff}] %newline[%-5level] :: [%logger(%property{NDC})] %newline%message%newline%exception%newline%newline" />
			</layout>
		</appender>
		<!-- Aquí saldría todo -->
		<appender name="InfoRollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
			<lockingModel type="LogAppender.MinimalLockDeleteEmpty" />
			<file value="C:\Log\AMPAExt\Log_" />
			<datePattern value="dd.MM.yyyy'.txt'" />
			<staticLogFileName value="false" />
			<appendToFile value="true" />
			<rollingStyle value="Composite" />
			<maxSizeRollBackups value="10" />
			<maximumFileSize value="10MB" />
			<layout type="log4net.Layout.PatternLayout">
				<param name="ConversionPattern" value="[%date{dd/MM/yyyy HH:mm:ss fff}] %newline[%-5level] :: [%logger(%property{NDC})] %newline%message%newline%exception%newline%newline" />
			</layout>
		</appender>
	</log4net>
